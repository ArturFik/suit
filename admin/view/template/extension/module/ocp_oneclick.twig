                                    </div>
                                  </div>
                                  <div class="form-group required">
                                      <label class="col-sm-4 control-label"><span data-toggle="tooltip" data-placement="right" title="{{ tip_list_btn_class }}">{{ entry_list_btn_class }}</span></label>
                                    <div class="col-sm-8">
                                        <input type="text" name="ocp_oneclick_list_btns[{{ loop.index }}][list_btn_class]" class="form-control" value="{{ list_btn.list_btn_class }}" placeholder="btn-ocpoc" />
                                    </div>
                                  </div>
                                  <div class="form-group">
                                      <label class="col-sm-4 control-label"><span data-toggle="tooltip" data-placement="right" title="{{ tip_list_btn_css }}">{{ entry_list_btn_css }}</span></label>
                                    <div class="col-sm-8">
                                        <textarea name="ocp_oneclick_list_btns[{{ loop.index }}][list_btn_css]" class="form-control">{{ list_btn.list_btn_css }}</textarea>
                                    </div>
                                  </div>
                            </div>
                            {% endfor %}
                            </div>
                            </div>
                        </div>

                    </div>
                    <div class="tab-pane" id="tab-btn-product">
                      <div class="form-group required">
                          <label class="col-sm-3 control-label"><span data-toggle="tooltip" data-placement="right" title="{{ tip_product_element }}">{{ entry_product_element }}</span></label>
                        <div class="col-sm-9">
                          <input type="text" name="ocp_oneclick_settings[product_element]" class="form-control" value="{{ settings.product_element }}" placeholder=".class" />
