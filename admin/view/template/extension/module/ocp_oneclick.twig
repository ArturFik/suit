                      <input type="radio" name="ocp_oneclick_settings[show_images]" value="0" autocomplete="off"{{ settings.show_images  == 0 ? ' checked="checked"' : '' }} />{{ text_no }}
                    </label>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <label class="col-sm-3 control-label">{{ entry_show_options }}</label>
