                      <input type="radio" name="ocp_oneclick_settings[show_qty]" value="1" autocomplete="off"{{ settings.show_qty  == 1 ? ' checked="checked"' : '' }} />{{ text_yes }}
                    </label>
                    <label class="btn btn-danger btn-default {{ settings.show_qty  == 0 ? 'active' : '' }}">
                      <input type="radio" name="ocp_oneclick_settings[show_qty]" value="0" autocomplete="off"{{ settings.show_qty  == 0 ? ' checked="checked"' : '' }} />{{ text_no }}
                    </label>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <label class="col-sm-3 control-label">{{ entry_stock_checkout }}</label>
                <div class="col-sm-9">
                  <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default {{ settings.stock_checkout  == 1 ? 'active' : '' }}">
                      <input type="radio" name="ocp_oneclick_settings[stock_checkout]" value="1" autocomplete="off"{{ settings.stock_checkout  == 1 ? ' checked="checked"' : '' }} />{{ text_yes }}
                    </label>
                    <label class="btn btn-danger btn-default {{ settings.stock_checkout  == 0 ? 'active' : '' }}">
                      <input type="radio" name="ocp_oneclick_settings[stock_checkout]" value="0" autocomplete="off"{{ settings.stock_checkout  == 0 ? ' checked="checked"' : '' }} />{{ text_no }}
                    </label>
                  </div>
                </div>
              </div>
              <div class="form-group">
                  <label class="col-sm-3 control-label">{{ entry_telephone_mask }}</label>
                <div class="col-sm-9">
                    <input name="ocp_oneclick_settings[telephone_mask]" class="form-control" value="{{ settings.telephone_mask }}" />
                </div>
              </div>
              <div class="form-group">
                  <label class="col-sm-3 control-label">{{ entry_agree }}</label>
                <div class="col-sm-9">
                    <select name="ocp_oneclick_settings[agree]" class="form-control">
                        <option value="">{{ text_not_required }}</option>
                        {% for info in informations %}
                        <option value="{{ info.information_id }}"{{ settings.agree == info.information_id ? ' selected="selected"' : '' }}>{{ info.name }}</option>
                        {% endfor %}
                    </select>
                </div>
              </div>
              <div class="form-group">
                  <label class="col-sm-3 control-label">{{ entry_agree_type }}</label>
                <div class="col-sm-9">
                    <select name="ocp_oneclick_settings[agree_type]" class="form-control">
                        <option value="0"{{ settings.agree_type == 0 ? ' selected="selected"' : '' }}>{{ text_agree_text }}</option>
                        <option value="1"{{ settings.agree_type == 1 ? ' selected="selected"' : '' }}>{{ text_agree_checkbox }}</option>
                    </select>
                </div>
              </div>
            </div>
            <div class="tab-pane" id="tab-btns">
                  <ul class="nav nav-tabs" id="btns_tabs">
                      <li class="active"><a href="#tab-btn-list" data-toggle="tab">{{ tab_btn_list }}</a></li>
                      <li><a href="#tab-btn-product" data-toggle="tab">{{ tab_btn_product }}</a></li>
                  </ul>
                  <div class="tab-content">
                    <div class="tab-pane active" id="tab-btn-list">

                        <div class="row">
                            <div class="col-sm-2">
                                <ul class="nav nav-pills nav-stacked" id="list_btns">
                                    {% for list_btn in list_btns %}
                                    <li{% if loop.index == 1 %} class="active"{% endif %}><a href="#tab-btn{{ loop.index }}" data-toggle="tab" aria-expanded="true"><i class="fa fa-minus-circle" onclick="$('a[href=\'#tab-btn{{ loop.index }}\']').parent().remove(); $('#tab-btn{{ loop.index }}').remove(); $('#list_btns a:first').tab('show');"></i> {{ list_btn.list_btn_name?list_btn.list_btn_name:loop.index }}</a></li>
                                    {% endfor %}

                                </ul>
                                <br />
                                <button type="button" onclick="addListBtn();" data-toggle="tooltip" title="" class="btn" data-original-title="Добавить"><i class="fa fa-plus-circle"></i> Добавить кнопку</button>
                            </div>
                            <div class="col-sm-10">
                                <div class="tab-content">
                                {% for list_btn in list_btns %}
                                <div class="tab-pane{% if loop.index==1 %} active{% endif %}" id="tab-btn{{ loop.index }}">
                                  <div class="form-group">
                                      <label class="col-sm-4 control-label"><span data-toggle="tooltip" data-placement="right" title="{{ tip_list_btn_name }}">{{ entry_list_btn_name }}</span></label>
                                    <div class="col-sm-8">
                                        <input type="text" name="ocp_oneclick_list_btns[{{ loop.index }}][list_btn_name]" class="form-control" value="{{ list_btn.list_btn_name }}" />
                                    </div>
                                  </div>
                                  <div class="form-group required">
                                      <label class="col-sm-4 control-label"><span data-toggle="tooltip" data-placement="right" title="{{ tip_list_product_block }}">{{ entry_list_product_block }}</span></label>
                                    <div class="col-sm-8">
                                        <input type="text" name="ocp_oneclick_list_btns[{{ loop.index }}][list_product_block]" class="form-control" value="{{ list_btn.list_product_block }}" placeholder=".class" />
                                    </div>
                                  </div>
                                  <div class="form-group required">
                                      <label class="col-sm-4 control-label"><span data-toggle="tooltip" data-placement="right" title="{{ tip_list_element }}">{{ entry_list_element }}</span></label>
                                    <div class="col-sm-8">
                                      <input type="text" name="ocp_oneclick_list_btns[{{ loop.index }}][list_element]" class="form-control" value="{{ list_btn.list_element }}" placeholder=".class" />
                                    </div>
                                  </div>
                                  <div class="form-group">
                                      <label class="col-sm-4 control-label"><span data-toggle="tooltip" data-placement="right" title="{{ tip_list_position }}">{{ entry_list_position }}</span></label>
                                    <div class="col-sm-8">
                                        <select name="ocp_oneclick_list_btns[{{ loop.index }}][list_position]" class="form-control">
                                            <option value="1"{{ list_btn.list_position  == 1 ? ' selected="selected"' : '' }}>{{ text_before_element }}</option>
                                            <option value="2"{{ list_btn.list_position  == 2 ? ' selected="selected"' : '' }}>{{ text_after_element }}</option>
                                        </select>
                                    </div>
                                  </div>
                                  <div class="form-group required">
                                      <label class="col-sm-4 control-label"><span data-toggle="tooltip" data-placement="right" title="{{ tip_list_btn_block_class }}">{{ entry_list_btn_block_class }}</span></label>
                                    <div class="col-sm-8">
                                        <input type="text" name="ocp_oneclick_list_btns[{{ loop.index }}][list_btn_block_class]" class="form-control" value="{{ list_btn.list_btn_block_class }}" placeholder="btn-ocpoc-wrap" />
                                    </div>
                                  </div>
                                  <div class="form-group">
                                      <label class="col-sm-4 control-label"><span data-toggle="tooltip" data-placement="right" title="{{ tip_list_btn_block_css }}">{{ entry_list_btn_block_css }}</span></label>
                                    <div class="col-sm-8">
