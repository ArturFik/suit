                  <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default {{ settings.show_options  == 1 ? 'active' : '' }}">
                      <input type="radio" name="ocp_oneclick_settings[show_options]" value="1" autocomplete="off"{{ settings.show_options  == 1 ? ' checked="checked"' : '' }} />{{ text_yes }}
