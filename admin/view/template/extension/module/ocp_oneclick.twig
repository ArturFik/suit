            <div class="tab-pane" id="tab-localisation">
                <ul class="nav nav-tabs" id="localisation_tabs">
                    {% for language in languages %}
                      <li>
                        <a href="#tab-localisation-language{{ language.language_id }}" data-toggle="tab"><img src="{{ language.image }}" title="{{ language.name }}" /> {{ language.name }}</a>
                      </li>
                    {% endfor %}
                  </ul>
                <div class="tab-content">
