                    <label class="btn btn-danger btn-default {{ settings.show_options  == 0 ? 'active' : '' }}">
                      <input type="radio" name="ocp_oneclick_settings[show_options]" value="0" autocomplete="off"{{ settings.show_options  == 0 ? ' checked="checked"' : '' }} />{{ text_no }}
